<div class="container-fluid">
  <h2>Users Table</h2>
  <table class="table">
    <thead>
      <tr>
        <th>User</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to 'New User', new_user_path %>
</div>
